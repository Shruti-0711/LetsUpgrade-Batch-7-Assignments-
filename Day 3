        "alt = input(\"Enter Plane Altitude: \") \n",
        "print(\"Your Entered Altitude Is:\" , alt , \"Feet\")\n",
        "a = float(alt)\n",
        "if a > 0:\n",
        "  if a == 1000:\n",
        "      print(\"Safe To Land\")\n",
        "  if a > 1000:\n",
        "      print(\"It is Greater Than 1000\")\n",
        "      if a < 5000:\n",
        "        print(\"Bring Down To 1000\")\n",
        "      else:\n",
        "        print(\"Turn Around\")\n",
        "  elif a < 1000:\n",
        "    print(\"Land The Plane\")\n",
        "else:\n",
        "    print(\"Altitude Must Be Positive Integer\")\n",
