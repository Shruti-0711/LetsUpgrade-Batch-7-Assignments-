        "ll = input(\"Enter Lower Value: \") \n",
        "ul = input(\"Enter Upper Value: \") \n",
        "\n",
        "ll=int(ll)\n",
        "ul=int(ul)\n",
        "print(\"Prime numbers between\", ll, \"&\", ul, \"are:\")\n",
        "\n",
        "for p in range(ll, ul + 1):\n",
        "   \n",
        "   if p > 1:\n",
        "       for i in range(2, p):\n",
        "           if (p % i) == 0:\n",
        "               break\n",
        "       else:\n",
        "           print(p)"
